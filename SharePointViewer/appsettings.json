{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SharePointAuth": {
    "AuthType": "ClientSecret", // Options: "ClientSecret" or "UsernamePassword"
    "TenantId": "YOUR_TENANT_ID",
    "ClientId": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET",
    "Username": "your.user@yourdomain.onmicrosoft.com",
    "Password": "YOUR_PASSWORD"
  }
}
